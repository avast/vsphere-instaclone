  <bean class="com.avast.teamcity.plugins.instaclone.buildstep.VSphereInstacloneRunType"/>
  <bean class="com.avast.teamcity.plugins.instaclone.CreateInstaCloneDetector"/>
  <bean class="com.avast.teamcity.plugins.instaclone.web.ICCloudProfileSettings"/>
  <bean class="com.avast.teamcity.plugins.instaclone.web.service.CloudProfilesService" lazy-init="true"/>
  <bean class="com.avast.teamcity.plugins.instaclone.web.ICVCenterAccountController"/>
  <bean class="com.avast.teamcity.plugins.instaclone.web.ICProfileController"/>
